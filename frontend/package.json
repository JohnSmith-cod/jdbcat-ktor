{
  "name": "frontend",
  "version": "1.0.0",
  "description": "JDBCat+Ktor Frontend",
  "main": "index.js",
  "scripts": {
    "dev": "parcel ./src/index.html",
    "watch": "parcel watch ./src/index.html --out-dir ../src/main/resources/frontend --public-url /frontend",
    "build": "parcel build ./src/index.html --out-dir ../src/main/resources/frontend --public-url /frontend",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'"
  },
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.0.0"
  },
  "keywords": [
    "JDBCat",
    "ktor",
    "React"
  ],
  "author": "Toly Pochkin",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "helpful-decorators": "^1.7.2",
    "jquery": "1.9.1",
    "popper.js": "^1.14.3",
    "react": "^16.5.2",
    "react-bootstrap": "^1.0.0-beta.1",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.10.0",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.5.2",
    "react-testing-library": "^5.1.0",
    "ts-jest": "^23.10.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3"
  }
